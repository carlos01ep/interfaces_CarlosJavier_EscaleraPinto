<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graficos.VentanaGraficosController">
   <children>
      <LineChart fx:id="lineChart" title="Número de entradas vendidas">
        <xAxis>
          <CategoryAxis label="Tipo de entrada /meses del año" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis lowerBound="1.0" minorTickCount="1" side="LEFT" tickUnit="1.0" upperBound="10.0" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
